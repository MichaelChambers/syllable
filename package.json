{
  "name": "syllable",
  "version": "3.0.0",
  "description": "Count syllables in English words",
  "license": "MIT",
  "keywords": [
    "detect",
    "syllables",
    "natural",
    "language",
    "cli",
    "bin"
  ],
  "homepage": "https://words.github.io/syllable/",
  "repository": "https://github.com/words/syllable",
  "bugs": "https://github.com/words/syllable/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "problematic.json",
    "index.js",
    "cli.js"
  ],
  "bin": "cli.js",
  "dependencies": {
    "normalize-strings": "^1.1.0",
    "pluralize": "^7.0.0",
    "trim": "0.0.1"
  },
  "devDependencies": {
    "browserify": "^14.0.0",
    "esmangle": "^1.0.0",
    "execa": "^0.8.0",
    "nyc": "^11.0.0",
    "remark-cli": "^4.0.0",
    "remark-preset-wooorm": "^3.0.0",
    "tape": "^4.4.0",
    "xo": "^0.18.0"
  },
  "scripts": {
    "build-md": "remark . -qfo",
    "build-bundle": "browserify index.js -s syllable > syllable.js",
    "build-mangle": "esmangle syllable.js > syllable.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test/index.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "esnext": false,
    "rules": {
      "guard-for-in": "off"
    },
    "ignores": [
      "syllable.js"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
